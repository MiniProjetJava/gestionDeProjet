<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.SDIA.gestiondeprojet.presentation.controllers.ResponsableMenuControllers">
   <top>
      <TabPane tabClosingPolicy="UNAVAILABLE" BorderPane.alignment="CENTER">
        <tabs>
              <Tab text="Gestion des intervenants">
                <content>
                   <fx:include source="gestionIntervenantInterface.fxml" />
                </content>
              </Tab>
              <Tab text="Gestion des informations personnelles">
                <content>
                  <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0" />
                </content>
              </Tab>
                <Tab text="Gestion des taches">
                <content>
                    <fx:include source="gestionTACHES.fxml" />
                </content>
                </Tab>
        </tabs>
      </TabPane>
   </top>
</BorderPane>
